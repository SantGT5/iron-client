      <h1 className=\"mb-2\">Order Summary</h1>\n      <div className=\"list-group\">\n        {state.map((product) => {\n          return (\n            <Link\n              key={product._id}\n              to={`/productDetails/${product._id}`}\n              className=\"list-group-item list-group-item-action\"\n            >\n              <div className=\"d-flex w-100 justify-content-between row\">\n                <div className=\"col-4\">\n                  <img\n                    className=\"mw-100\"\n                    src={product.image_url}\n                    alt={product.name}\n                  />\n                </div>\n\n                <div className=\"col-8\">\n                  <h5 className=\"mb-1\">{product.name}</h5>\n                  <h3>\n                    {product.price.toLocaleString(\n                      window.navigator.languages[0],\n                      { style: \"currency\", currency: \"USD\" }\n                    )}\n                  </h3>\n                  <small>Quantity: {product.qtt}</small>\n                </div>\n              </div>\n            </Link>\n          );\n        })}\n\n        <button className=\"btn btn-primary btn-lg mt-3\" onClick={handleSubmit}>\n          Confirm Order\n        </button>\n      </div>\n    </div>\n  );\n}\n\nexport default Checkout;\n\n",{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":12,"column":25,"nodeType":"107","messageId":"108","endLine":12,"endColumn":31},{"ruleId":"105","severity":1,"message":"109","line":16,"column":11,"nodeType":"107","messageId":"108","endLine":16,"endColumn":23},"no-native-reassign",["110"],"no-negated-in-lhs",["111"],"no-unused-vars","'logoff' is assigned a value but never used.","Identifier","unusedVar","'loggedInUser' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]
