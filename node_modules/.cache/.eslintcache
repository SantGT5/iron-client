      >\n            <option>Selecione o Tamanho</option>\n            <option value=\"310\">310</option>\n            <option value=\"330\">330</option>\n            <option value=\"355\">355</option>\n            <option value=\"500\">500</option>\n            <option value=\"600\">600</option>\n          </select>\n      </div>\n    </div>\n    <TextInput\n        label=\"Preço\"\n        id=\"price\"\n        type=\"number\"\n        value={state.price}\n        onChange={handleChange}\n        name=\"price\"\n        required\n      />\n      <div className=\"form-group\">\n      <form action=\"/action_page.php\">\n    <label htmlFor=\"img\">Select image:</label>\n    <input type=\"file\" id=\"img\" name=\"img\" accept=\"image/*\"></input>\n    </form>\n        <button className=\"btn btn-primary\" type=\"submit\" >\n          Salvar\n        </button>\n      </div>\n      </form>\n  );\n}\n\nexport default EditProduct;","/mnt/c/ironhack/e-comerce/iron-client/src/routerComponents/auth/Profile.js",[],"/mnt/c/ironhack/e-comerce/iron-client/src/routerComponents/auth/Signup.js",[],"/mnt/c/ironhack/e-comerce/iron-client/src/routerComponents/account/EditeUser.js",[],"/mnt/c/ironhack/e-comerce/iron-client/src/Products/ProductDetail.js",["108"],"import React, { useEffect, useState, useContext } from \"react\";\nimport api from \"../apis/api\";\nimport { useParams, Link } from \"react-router-dom\";\nimport CardProducts from \"../components/CardProducts\";\nimport { CartContext } from \"../contexts/cartContext\";\nimport { AuthContext } from \"../contexts/authContext\";\n\nfunction ProductDetails() {\n  const [state, setState] = useState({\n    userid: [],\n    name: \"\",\n    description: \"\",\n    size: \"\",\n    price: \"\",\n    id:\"\",\n    img: \"\"\n  });\n  const { loggedInUser, logoff } = useContext(AuthContext);\n  const [quantity, setQuantity] = useState(0);\n  const { id } = useParams();\n  const { cart, setCart } = useContext(CartContext);\n\n  useEffect